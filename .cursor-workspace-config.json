      "**/bin/**": true,
      "**/obj/**": true,
      "**/node_modules/**": true,
      "**/.angular/**": true,
      "**/dist/**": true
    },
    "files.exclude": {
      "**/bin/**": true,
      "**/obj/**": true
    }
  }
}
