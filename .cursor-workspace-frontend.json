{
  "name": "Drashyam - Frontend Only",
  "folders": [
    {
      "path": "./Frontend"
    }
  ],
  "settings": {
    "files.watcherExclude": {
      "**/.git/objects/**": true,
      "**/node_modules/**": true,
      "**/.angular/**": true,
      "**/dist/**": true,
      "**/coverage/**": true
    },
    "search.exclude": {
      "**/node_modules/**": true,
      "**/.angular/**": true,
      "**/dist/**": true,
      "**/coverage/**": true
    },
    "typescript.preferences.includePackageJsonAutoImports": "off",
    "typescript.suggest.autoImports": false
  }
}
